    "core-js": "^3.19.3",
    "nuxt": "^2.15.8",
    "nuxt-property-decorator": "^2.9.1",
    "postcss": "^8.4.16",
    "vue": "^2.6.14",
    "vue-chartjs": "^3.5.1",
    "vue-class-component": "^7.2.6",
    "vue-server-renderer": "^2.6.14",
    "vue-template-compiler": "^2.6.14",
    "vuetify": "^2.6.1",
