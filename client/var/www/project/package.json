    "eslint": "^8.4.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-nuxt": "^3.1.0",
    "eslint-plugin-vue": "^8.2.0",
    "jest": "^27.4.4",
    "node-sass": "^4.14.1",
    "prettier": "^2.5.1",
    "sass-loader": "^10.1.1",
    "ts-jest": "^27.1.1",
